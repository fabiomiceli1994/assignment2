#!/bin/bash
reset
set terminal pdf
set output 'ADRsol1.pdf'
set xlabel "j"
set ylabel "u(x_{j})"
set key right bottom
#set logscale y
#set xrange [0:550]
#set logscale xy
plot 'P_numb_0.000500_Solution_J_10.txt' using 1:2 w l title "{P_e}=0.0005, J=10", \
